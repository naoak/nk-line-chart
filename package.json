{
  "name": "nk-line-chart",
  "version": "2.3.0",
  "description": "A very lightweight polymer component for drawing line charts.",
  "keywords": [
    "web-components",
    "polymer",
    "chart"
  ],
  "homepage": "https://github.com/naoak/nk-line-chart",
  "repository": {
    "type": "git",
    "url": "git://github.com/naoak/nk-line-chart"
  },
  "license": "MIT",
  "author": "Naoaki Yamada",
  "main": "nk-line-chart.js",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "dependencies": {
    "@polymer/decorators": "^3.0.0",
    "@polymer/polymer": "^3.0.0"
  },
  "devDependencies": {
    "@open-wc/building-webpack": "^2.13.37",
    "@open-wc/testing": "^2.5.19",
    "@open-wc/testing-karma": "^3.4.7",
    "@polymer/iron-demo-helpers": "^3.0.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "http-server": "^0.12.3",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "ts-loader": "^8.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "scripts": {
    "clean": "rimraf *.d.ts *.tgz nk-line-chart.js* nk-line-chart.spec.js*",
    "build": "tsc -d",
    "demo": "webpack-dev-server --mode development --open --host 0.0.0.0 --port 8081 --disable-host-check",
    "test": "tsc && karma start --coverage",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:watch": "tsc-watch --onSuccess \"concurrently 'karma start --coverage --auto-watch=true --single-run=false' 'http-server'\""
  }
}
